<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
                
    <bean id="car" class="com.tasif.setterobjects2.Car">  
		<property name="fuelType" value="Diesel"></property>  
		<property name="maxSpeed" value="100KMPH"></property>  
	</bean>  
  
	<bean id="bike" class="com.tasif.setterobjects2.Bike">  
		<property name="fuelType" value="Petrol"></property>  
		<property name="maxSpeed" value="150KMPH"></property>  
	</bean> 
	          
    <bean id="travel" class="com.tasif.setterobjects2.Travel">  
		<property name="v" ref="car"> </property>  
	</bean>                 
                
                
</beans>