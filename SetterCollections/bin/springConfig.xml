<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  
                
    <bean id="emp" class="com.tasif.spring.Employee">  
		<property name="employeeList">
		<list>
			<value>Tasif Mohammed</value>
			<value>Rajesh Singh</value>
			<value>Sanjeev Anand</value>
		</list>
		</property> 

		<property name="employeeSet">
		<set>
			<value>Tasif Mohammed</value>
			<value>Rajesh Singh</value>
			<value>Sanjeev Anand</value>
		</set>
		</property>  
		
		<property name="employeeMap">
		<map>
			<entry key = "1" value = "Tasif Mohammed"/>
			<entry key = "2" value = "Rajesh Singh"/>
			<entry key = "3" value = "Sanjeev Anand"/>
		</map>
		</property>  
	</bean>                  
</beans>