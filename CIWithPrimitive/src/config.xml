<?xml version="1.0" encoding="UTF-8"?>  
<beans  
    xmlns="http://www.springframework.org/schema/beans"  
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
    xmlns:p="http://www.springframework.org/schema/p"  
    xsi:schemaLocation="http://www.springframework.org/schema/beans  
                http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">  

	<bean id="employee" class="com.tasif.spring.Employee">
		<constructor-arg index="0" type="java.lang.String" value="Tasif"/>
		<constructor-arg index="1" type="long" value="917825"/>	
	</bean>   
	<bean id="employee1" class="com.tasif.spring.Employee">
		<constructor-arg index="0" type="java.lang.String" value="Tasif"/>
		<constructor-arg index="1" type="int" value="100"/>	
	</bean>   
	<bean id="employee2" class="com.tasif.spring.Employee">
		<constructor-arg index="0" type="java.lang.String" value="Tasif"/>
		<constructor-arg index="1" type="int" value="100"/>	
		<constructor-arg index="2" type="long" value="917825"/>	
	</bean>            
              
</beans>