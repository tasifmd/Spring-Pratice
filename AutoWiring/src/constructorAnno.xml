<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context-3.0.xsd">
 
    <context:annotation-config />   
	
	<bean id="employeeAnnoConst" class="com.tasif.annotation.EmployeeAnnoConst">
		<property name="id" value="100"/>
		<property name="name" value="Tasif"/>
	</bean>
	<bean id="addressAnnoConst" class="com.tasif.annotation.AddressAnnoConst">
		<property name="city" value="Mumbai"/>
		<property name="state" value="Maharastra"/>
	</bean>
	
</beans>