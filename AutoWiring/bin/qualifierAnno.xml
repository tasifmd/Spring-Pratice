<?xml version="1.0" encoding="UTF-8"?>  
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">
    <context:annotation-config/>
    
	<bean id="employee" class="com.tasif.qualifier.EmployeeQualifier">
		<property name="name" value="Tasif"/>
		<property name="id" value="100"/>
		
	</bean>
	<bean id="address1" class="com.tasif.qualifier.AddressQualifier">
		<property name="city" value="Mumbai"/>
		<property name="state" value="Maharastra"/>
	</bean>
	<bean id="address2" class="com.tasif.qualifier.AddressQualifier">
		<property name="city" value="Bangalore"/>
		<property name="state" value="Karnataka"/>
	</bean>
	
	
	
</beans>